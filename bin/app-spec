#!/usr/bin/env perl
# vim:et:sts=4:sws=4:sw=4
use strict;
use warnings;
use 5.010;
use Data::Dumper;
use FindBin '$Bin';

use App::Spec;
use App::Spec::AppSpec;
use App::Spec::Run::AppSpec;

my $spec = App::Spec::AppSpec->read("$Bin/../etc/app-spec.yaml");
my $run = App::Spec::Run::AppSpec->new({
    spec => $spec,
});
$run->run;


